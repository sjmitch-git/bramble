[
	{
		"id": "amex",
		"name": "American Express",
		"validate": "(?:3[47][0-9]{13})",
		"hint": "Starting with 34 or 37, length 15 digits"
	},
	{
		"id": "dinersclub",
		"name": "Diners Club",
		"validate": "(?:3(?:0[0-5]|[68][0-9])[0-9]{11})",
		"hint": "Starting with 300 through 305, 36, or 38, length 14 digits"
	},
	{
		"id": "discover",
		"name": "Discover",
		"validate": "(?:6(?:011|5[0-9][0-9])[0-9]{12})",
		"hint": "Starting with 6011, length 16 digits or starting with 5, length 15 digits"
	},
	{
		"id": "jcb",
		"name": "JCB",
		"validate": "(?:(?:2131|1800|35/\\d{3})/\\d{11})",
		"hint": "Starting with 2131 or 1800, length 15 digits or starting with 35, length 16 digits"
	},
	{
		"id": "mastercard",
		"name": "MasterCard",
		"validate": "(?:5[1-5][0-9]{14})",
		"hint": "Starting with 51 through 55, length 16 digits"
	},
	{
		"id": "visa",
		"name": "Visa",
		"validate": "(?:4[0-9]{12}(?:[0-9]{3})?)",
		"hint": "Starting with 4 length 13 or 16 digits"
	}
]
